<div class="container-fluid">
    <div class="row">
    {{#each .}}
        <div class="col-xs-6">
            <div class="well">
                <div class="ad">
                    <div class="row">
                        <div class="col-md-6">
                            <h3>{{this._title}}</h3>
                            <img src="{{this._imageUrl}}" class="ad-image">
                        </div>
                        <div class="col-md-6">
                            <h3>{{this._make}} {{this._model}} {{this._price}}$</h3>
                            <p>Manufacture date: {{this._manufactureDate}}</p>
                            <p>Fuel: {{this._fuel}}</p>
                            <p>Power: {{this._power}} ps.</p>
                            <p>Mileage: {{this._mileage}} km.</p>
                            <p>Gearbox: {{this._gearbox}}</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
    </div>
</div>